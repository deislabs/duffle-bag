{
  "name": "helloworld",
  "version": "0.1.1",
  "invocationImages":
    [
      {
        "imageType": "docker",
        "image": "cnab/helloworld:0.1.1"
      }
    ],
  "images": [],
  "parameters": {
    "port": {
      "defaultValue": 8080,
      "type": "int"
    },
    "somebool": {
      "type": "bool"
    },
    "favouriteWomble": {
      "type": "string",
      "allowedValues": [
        "Great Uncle Bulgaria",
        "Orinoco",
        "Madame Cholet",
        "Tomsk"
      ],
      "defaultValue": "Orinoco",
      "metadata": {
        "description": "Used when going underground, going overground and/or wombling free"
      }
    }
  },
  "credentials": {
    "secret_password": {
      "env": "so_very_secret"
    },
    "secret_cert": {
      "path": "/so/very/secret"
    }
  }
}
