{
  "name": "helloworld",
  "version": "0.1.1",
  "invocationImages": [
      {
        "imageType": "docker",
        "image": "cnab/helloworld:0.1.1"
      }
    ],
  "images": [
    {
      "imageType": "docker",
      "image": "busybox"
    }
  ],
  "parameters": {
    "port": {
      "defaultValue": 8080,
      "type": "int",
      "minValue": 1000,
      "maxValue": 9999
    },
    "region": {
      "defaultValue": "Wimbledon Common",
      "type": "string",
      "minLength": 4,
      "maxLength": 20
    },
    "somebool": {
      "type": "bool"
    },
    "favouriteWomble": {
      "type": "string",
      "allowedValues": [
        "Great Uncle Bulgaria",
        "Orinoco",
        "Madame Cholet",
        "Tomsk"
      ],
      "defaultValue": "Orinoco",
      "metadata": {
        "description": "Used when going underground, going overground and/or wombling free"
      }
    }
  },
  "credentials": {
    "secret_password": {
      "env": "so_very_secret"
    },
    "secret_cert": {
      "path": "/so/very/secret"
    }
  }
}
