{
  "name": "spline-reticulator",
  "version": "0.1.1",
  "invocationImages": [
      {
        "imageType": "docker",
        "image": "cnab/helloworld:0.1.1"
      }
    ],
  "images": [
    {
      "imageType": "docker",
      "image": "busybox"
    }
  ],
  "parameters": {
    "port": {
      "defaultValue": 8080,
      "type": "int",
      "minValue": 1000,
      "maxValue": 9999
    },
    "region": {
      "defaultValue": "New Zealand South",
      "type": "string",
      "minLength": 4,
      "maxLength": 24
    },
    "public": {
      "type": "bool"
    },
    "reticulationMode": {
      "type": "string",
      "allowedValues": [
        "Reticulate only",
        "Reticulate and frobnicate",
        "Quantum reticulation",
        "Quantum reticulation with bicategorical induction"
      ],
      "defaultValue": "Reticulate only",
      "metadata": {
        "description": "How to perform the reticulation. Quantum options require NIY (Not Invented Yet) processor at minimum."
      }
    }
  },
  "credentials": {
    "secret_password": {
      "env": "so_very_secret"
    },
    "secret_cert": {
      "path": "/so/very/secret"
    }
  }
}
